<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.mission.assign.AssignPageController">
   <children>
      <TabPane layoutY="1.0" prefHeight="768.0" prefWidth="1366.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <tabs>
          <Tab fx:id="flightPathTab" onSelectionChanged="#selected" text="Flight Path">
            <content>
              <fx:include fx:id="flightPathPage" source="FlightPathPage.fxml" />
            </content>
          </Tab>
          <Tab fx:id="flighDetailsTab" onSelectionChanged="#selected" text="Flight Details">
            <content>
               <fx:include fx:id="flighDetails" source="FlightDetails.fxml" />
            </content>
          </Tab>
            <Tab fx:id="flightReviewTab" onSelectionChanged="#selected" text="Review/Confirm">
               <content>
                  <fx:include fx:id="flightReview" source="FlightReviewPage.fxml" />
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
